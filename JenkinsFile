node {
  
     def mvnHome = tool name: 'MAVEN_HOME', type: 'maven'
    stage('Clone sources') {
        git url: 'https://github.com/happymark12/Test.git'
    }
    
    stage('Maven build') {
       
        bat script: "${mvnHome}/bin/mvn clean compile"
    }

    stage('Test') {
         bat script: "${mvnHome}/bin/mvn test"
    }
}
